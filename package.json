{
  "devDependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.8",
    "@vitest/coverage-c8": "^0.24.1",
    "eslint": "^8.25.0",
    "nazna": "^1.3.5",
    "nazna-tsconfig": "^1.0.0",
    "pnpm": "^7.13.4",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  },
  "peerDependencies": {
    "@morphic-ts/batteries": "^3.0.0",
    "@morphic-ts/summoners": "^3.0.0"
  },
  "scripts": {
    "build": "pnpm build:types && pnpm build:es6 && pnpm build:cjs && nazna build cli",
    "fix": "eslint --max-warnings=0 --ext .ts . --fix",
    "lint": "eslint --max-warnings=0 --ext .ts .",
    "test": "vitest",
    "build:es6": "swc src --out-dir dist/es6 --source-maps",
    "build:cjs": "swc src --out-dir dist/cjs --source-maps --config module.type=commonjs",
    "build:types": "tsc src/**.ts --outDir dist/types --skipLibCheck --declaration --declarationMap --emitDeclarationOnly --esModuleInterop",
    "postinstall": "nazna fix"
  },
  "name": "make-union-morphic-ts",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/chimeraant/nazna.git",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es6/index.js"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/cli.js"
}
